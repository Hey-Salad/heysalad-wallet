{
  "expo": {
    "name": "HeySalad速 Wallet",
    "slug": "heysalad-wallet",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "heysalad",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heysalad.wallet",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSMicrophoneUsageDescription": "HeySalad速 Wallet uses the microphone for voice payments with Selina.",
        "NSFaceIDUsageDescription": "HeySalad速 Wallet uses Face ID to securely authorize transactions.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "HeySalad速 Wallet uses the camera to scan QR codes.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.heysalad.wallet",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false,
            "deploymentTarget": "15.1"
          },
          "android": {
            "newArchEnabled": false,
            "minSdkVersion": 24
          }
        }
      ],
      "@config-plugins/react-native-webrtc",
      "@livekit/react-native-expo-plugin",
      "expo-router",
      "expo-av",
      "expo-local-authentication",
      "expo-secure-store",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "909b7fa0-fae8-4b5a-a474-b0dedfb5c73a"
      }
    },
    "owner": "heysalad"
  }
}