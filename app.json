{
  "expo": {
    "name": "HeySalad® Wallet",
    "slug": "heysalad-wallet",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heysalad.wallet",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "NSMicrophoneUsageDescription": "Allow HeySalad® Wallet to access your microphone for voice payments",
        "NSFaceIDUsageDescription": "Allow HeySalad® Wallet to use Face ID to securely authorize transactions and protect your private keys.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.heysalad.wallet",
      "permissions": [
        "RECORD_AUDIO",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow HeySalad® Wallet to access your microphone for voice payments"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow HeySalad® Wallet to use Face ID to securely authorize transactions."
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow HeySalad® Wallet to access your Face ID biometric data to protect your private keys."
        }
      ],
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "909b7fa0-fae8-4b5a-a474-b0dedfb5c73a"
      }
    },
    "owner": "heysalad"
  }
}